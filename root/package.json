{
  "name": "discord.dev",
  "version": "1.0.0",
  "description": "Discord Application, specified in fetching Discord Previews commits and Discord status incidents.",
  "type": "module",
  "main": "dist/bot.js",
  "scripts": {
    "start": "node dist/bot.js",
    "build": "tsc",
    "dev": "nodemon --loader ts-node/esm src/bot.ts",
    "deploy": "ts-node src/deploy-commands.ts",
    "lint": "eslint . --ext .ts",
    "restart": "npm run stop && npm run start",
    "stop": "kill -SIGINT $(ps -ef | grep 'node dist\/bot.js' | awk '{print $2}')",
    "watch": "nodemon --loader ts-node/esm --project tsconfig.json src/bot.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "github",
    "typescript",
    "status-monitor"
  ],
  "author": "LushDashBlaze",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.6.5",
    "@discordjs/rest": "^1.7.1",
    "discord.js": "^14.17.3",
    "dotenv": "^16.3.1",
    "mongoose": "^7.5.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/node-fetch": "^3.2.1",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "nodemon": "^3.0.0"
  }
}
