{
  "name": "discord.dev",
  "version": "1.3.0",
  "description": "Discord Application, specified in fetching Discord Previews commits and Discord status incidents.",
  "type": "module",
  "main": "dist/bot.js",
  "scripts": {
    "start": "node dist/bot.js",
    "build": "tsc",
    "dev": "nodemon --loader ts-node/esm src/bot.ts",
    "deploy": "ts-node src/deploy-commands.ts",
    "lint": "eslint . --ext .ts",
    "restart": "npm run stop && npm run start",
    "stop": "kill -SIGINT $(ps -ef | grep 'node dist/bot.js' | awk '{print $2}')",
    "watch": "nodemon --loader ts-node/esm --project tsconfig.json src/bot.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "github",
    "typescript",
    "status-monitor",
    "utility"
  ],
  "author": "LushDashBlaze",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.12.2",
    "@discordjs/rest": "^2.6.0",
    "@napi-rs/canvas": "^0.1.78",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "discord-api-types": "^0.38.29",
    "discord.js": "^14.23.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongoose": "^8.19.1",
    "node-fetch": "^3.3.2",
    "tweetnacl": "^1.0.3",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/date-fns": "^2.6.3",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.0",
    "@types/node-fetch": "^2.6.13",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
